<?xml version="1.0" encoding="utf-8"?>

<TableLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_gravity="center_horizontal">
	
	<TextView
		style="@style/solverLabelStyle"
		android:text="Physical Model:">
	</TextView>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="10dp">
		
		<TextView
			android:text="F:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/forceView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal">
	
		<TextView
			android:text="Pow:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/powerOutputView"
			style="@style/solverDataRightStyle"
			android:layout_marginBottom="20dp">
		</TextView>
		
	</TableRow>
	
	<TextView
		style="@style/solverLabelStyle"
		android:text="Parameters:">
	</TextView>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="10dp">
	
		<TextView
			android:text="A1:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/a1View"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="A2:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/a2View"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="v:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/velocityView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="h:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/headWindView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="r:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/rollingFrictionView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="g:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/hillGradeView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="wc:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/weightCyclistView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="wm:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/weightEquipmentView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
	<TableRow
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	
		<TextView
			android:text="t:"
			style="@style/solverDataLeftStyle">
		</TextView>
		
		<TextView
			android:id="@+id/transmissionEfficiencyView"
			style="@style/solverDataRightStyle">
		</TextView>
		
	</TableRow>
	
</TableLayout>
